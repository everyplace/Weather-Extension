{
  "name": "Weather Extension",
  "description": "Superimposes the weather on to EVERY web page",
  "version": "0.0.1",  
  "content_scripts": [
    {
      "matches": ["http://*/*"],
      "css": ["style.css"],
      "js": ["jquery.js", "script.js"]
    }
  ],
  "manifest_version": 2  
}